{% extends "squelette.twig" %}

{% block content %}
<h1>Categorie - {{cat['libelle']}}</h1>
<h2>Description : {{cat['description']}}</h2>
<ul>
    {% for presta in prestations %}
        <li>
            <a href="{{ url_for('prestation', { 'id': presta['id'] }) }}">{{presta['libelle']}}</a>
        </li>
    {% endfor %}
</ul>
{% endblock %}